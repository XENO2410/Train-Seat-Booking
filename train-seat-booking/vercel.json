{
    "version": 2,
    "builds": [
      {
        "src": "dist/train-seat-booking/**/*",
        "use": "@vercel/static-build",
        "config": {
          "distDir": "dist/train-seat-booking"
        }
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "/index.html"
      }
    ]
  }